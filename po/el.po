msgid "Downloading"
msgstr "Γίνεται λήψη"

#: ../data/emblem-ubuntuone-unsynchronized.icon.in.h:1
msgid "Unsynchronized"
msgstr "Μη συγχρονισμένα"

#: ../data/emblem-ubuntuone-uploading.icon.in.h:1
msgid "Uploading"
msgstr "Γίνεται αποστολή"

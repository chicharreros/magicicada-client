#: ../ubuntuone/status/aggregator.py:51
msgid "Magicicada"
msgstr "Magicicada"

#: ../ubuntuone/status/aggregator.py:52
msgid "New cloud folder(s) available"
msgstr "Nowé katalodżi w chmùrze"

#: ../ubuntuone/status/aggregator.py:53
msgid "File synchronization completed."
msgstr "Synchronizacjô lopków ju sã zakùńczô."

#: ../ubuntuone/status/aggregator.py:55
#, python-format
msgid "%(percentage_completed)d%% completed."
msgstr "Ùkùńczono %(percentage_completed)d%%"

#: ../ubuntuone/status/aggregator.py:56
msgid "File synchronization in progress"
msgstr "Dérëje synchronizacjô lopków"

#: ../ubuntuone/status/aggregator.py:59
#, python-format
msgid ""
"There is no available space on the folder:\n"
"\"%s\" shared by %s"
msgstr ""
"Felëje placu w katalogù\n"
"\"%s\" ùprzistãpnionégò przez %s"

#: ../ubuntuone/status/aggregator.py:73
#, python-format
msgid "'%(filename)s' is being uploaded to your personal cloud."
msgstr "'%(filename)s' je wësëłany do chmùrë."

#: ../ubuntuone/status/aggregator.py:79
#, python-format
msgid ""
"'%(filename)s' and %(other_files)d other file are being uploaded to your "
"personal cloud."
msgid_plural ""
"'%(filename)s' and %(other_files)d other files are being uploaded to your "
"personal cloud."
msgstr[0] ""
"Wësëłanié lopka „%(filename)s” ë %(other_files)d jinégò lopka do chmùrë."
msgstr[1] ""
"Wësëłanié lopka „%(filename)s” ë %(other_files)d jinëch lopków do chmùrë."
msgstr[2] ""
"Wësëłanié lopka „%(filename)s” ë %(other_files)d jinëch lopków do chmùrë."

#: ../ubuntuone/status/aggregator.py:90
#, python-format
msgid "'%(filename)s' is being downloaded to your computer."
msgstr "Scyganié lopka '%(filename)s'."

#: ../ubuntuone/status/aggregator.py:96
#, python-format
msgid ""
"'%(filename)s' and %(other_files)d other file are being downloaded to your "
"computer."
msgid_plural ""
"'%(filename)s' and %(other_files)d other files are being downloaded to your "
"computer."
msgstr[0] "Scyganié lopka „%(filename)s” ë %(other_files)d jinégò lopka."
msgstr[1] "Scyganié „%(filename)s” ë %(other_files)d jinëch lopków."
msgstr[2] "Scyganié „%(filename)s” ë %(other_files)d jinëch lopków."

#: ../ubuntuone/status/aggregator.py:107
#, python-format
msgid "'%(filename)s' was uploaded to your personal cloud."
msgstr "'%(filename)s' òstôł wësłóny do chmùrë."

#: ../ubuntuone/status/aggregator.py:113
#, python-format
msgid ""
"'%(filename)s' and %(other_files)d other file were uploaded to your personal "
"cloud."
msgid_plural ""
"'%(filename)s' and %(other_files)d other files were uploaded to your "
"personal cloud."
msgstr[0] ""
"Lopk  „%(filename)s” ë %(other_files)d jiny lopk òstôł wësłóny do chmùrë."
msgstr[1] ""
"Lopk  „%(filename)s” ë %(other_files)d jiné lopczi òstałë wësłóné do chmùrë."
msgstr[2] ""
"Lopk  „%(filename)s” ë %(other_files)d jinëch lopków òstało wësłónëch do "
"chmùrë."

#: ../ubuntuone/status/aggregator.py:124
#, python-format
msgid "'%(filename)s' was downloaded to your computer."
msgstr "'%(filename)s' Òstôł scygniãti na kòmpùtr."

#: ../ubuntuone/status/aggregator.py:130
#, python-format
msgid ""
"'%(filename)s' and %(other_files)d other file were downloaded to your "
"computer."
msgid_plural ""
"'%(filename)s' and %(other_files)d other files were downloaded to your "
"computer."
msgstr[0] ""
"'%(filename)s' ë %(other_files)d jiny lopk òstôł scygniãti na kòmpùtr."
msgstr[1] ""
"'%(filename)s' ë %(other_files)d jiné lopczi òstałë scygniãti na kòmpùtr."
msgstr[2] ""
"'%(filename)s' ë %(other_files)d jinëch lopków òstało scygniãtëch na kòmpùtr."

#: ../ubuntuone/status/aggregator.py:191
#, python-format
msgid "A file was just made public at %(new_public_url)s"
msgstr "Lopk òstôł ùpistãpniony pòd adresã %(new_public_url)s"

#: ../ubuntuone/status/aggregator.py:204
#, python-format
msgid "%(event_count)d file was just made public."
msgid_plural "%(event_count)d files were just made public."
msgstr[0] "%(event_count)d lopk òstôł ùprzistãpniony"
msgstr[1] "%(event_count)d lopczi òstôłë ùprzistãpnioné"
msgstr[2] "%(event_count)d lopków òstało ùprzistãpnionëch"

#: ../ubuntuone/status/aggregator.py:212
msgid "A file is no longer published"
msgstr "Lopk ju je ùprzistãpniony"

#: ../ubuntuone/status/aggregator.py:220
#, python-format
msgid "%(event_count)d file is no longer published."
msgid_plural "%(event_count)d files are no longer published."
msgstr[0] "%(event_count)d lopk ju dłëżi nie je ùprzistãpniony"
msgstr[1] "%(event_count)d lopczi ju dłëżi nie są ùprzistãpnioné"
msgstr[2] "%(event_count)d lopczi ju dłëżi nie są ùprzistãpnioné"

#: ../ubuntuone/status/aggregator.py:235
#, python-format
msgid "Found %(event_count)d new cloud folder."
msgid_plural "Found %(event_count)d new cloud folders."
msgstr[0] "W chmùrze nalôzł sã  %(event_count)d nowy katalog"
msgstr[1] "W chmùrze nalażłë sã  %(event_count)d nowé katalodżi"
msgstr[2] "W chmùrze nalezło sã %(event_count)d nowëch katalogów"

#: ../ubuntuone/status/aggregator.py:243
#, python-format
msgid ""
"New cloud folder available: '%(folder_name)s' shared by %(other_user_name)s"
msgstr ""
"W chmùrze pòkôzôł sã nowi katalog: '%(folder_name)s' ùprzistãpniony przez "
"%(other_user_name)s"

#: ../ubuntuone/status/aggregator.py:259
#, python-format
msgid "New cloud folder available: '%(folder_name)s'"
msgstr "W chmùrze pòkôzôł sã nowi katalog: '%(folder_name)s"

#: ../ubuntuone/status/aggregator.py:281
msgid "The connection to the server was lost."
msgstr "Sparłãczenié z serwerã òstało ùtraconé."

#: ../ubuntuone/status/aggregator.py:287
msgid "The connection to the server was restored."
msgstr "Sparłãczenié z serwerã òstało przëwróconé."

